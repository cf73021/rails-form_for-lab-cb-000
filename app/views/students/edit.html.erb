<h3>Student Form</h3>

<%= form_tag student_path(@student), method: "put" do %>
<label>First Name:</label><br>
<%= text_field_tag :student_first_name, @student.first_name %><br>

<label>Last Name:</label><br>
<%= text_field_tag :student_last_name, @student.last_name %><br>

<%= submit_tag "Update Student" %>
<% end %>
